 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=v1);
	Nand(a=v1, b=b, out=v2);
	Nand(a=a, b=v1, out=v3);
	Nand(a=v2, b=v3, out=out);
}
